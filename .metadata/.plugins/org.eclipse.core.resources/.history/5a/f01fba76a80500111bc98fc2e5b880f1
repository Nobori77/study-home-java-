package studyReview;

public class StudyCasting {
	public static void main(String[] args) {
//      "8.43" + 2.59를 더해서 10이 나오도록 출력하기
		
		double num1 = 0.0, num2 = 0.0, numResult = 0;
		int num3 = 0;
		String result = "";
		
		num1 = Double.parseDouble("8.43");
		num2 = (int)2.59;
		num3 = (int)num1;
		numResult = num2 + num3;
		
		result = "%d + %d = %d";
		
		System.out.printf(result, num2, num3, numResult);
	}
}
